<configure
    xmlns="http://namespaces.zope.org/zope">

  <utility
      name="rdfturtle"
      factory=".factory.RDFTurtleAnnotator"
      provides=".interfaces.IExposureFileAnnotator"
      />

  <utility
      name="rdfn3"
      factory=".factory.RDFn3Annotator"
      provides=".interfaces.IExposureFileAnnotator"
      />

  <utility
      name="rdfxml"
      factory=".factory.RDFxmlAnnotator"
      provides=".interfaces.IExposureFileAnnotator"
      />

  <!-- default view generators -->

  <utility
      name="safe_html"
      factory=".factory.HTMLDocViewGen"
      provides=".interfaces.IExposureFileDocViewGen"
      />

  <utility
      name="rest_to_html"
      factory=".factory.RSTDocViewGen"
      provides=".interfaces.IExposureFileDocViewGen"
      />

</configure>
