<?xml version="1.0"?>
<atcttool
    xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <!-- 
  This section defines all indexes for search criteria and sort order.
 -->
 <topic_indexes i18n:domain="pmr2.app">
  <index enabled="True" 
      name="pmr2_review_state" 
      friendlyName="PMR2 Exposure Review State"
      description="The review state of the exposure.">
   <criteria>ATSelectionCriterion</criteria>
   <criteria>ATListCriterion</criteria>
  </index>
  <index enabled="True" 
      name="pmr2_curation" 
      friendlyName="PMR2 Curation"
      description="Curation of this model.">
   <criteria>ATSelectionCriterion</criteria>
   <criteria>ATSimpleStringCriterion</criteria>
   <criteria>ATListCriterion</criteria>
  </index>
  <index enabled="True" 
      name="pmr2_keyword" 
      friendlyName="PMR2 Model Keyword"
      description="Keyword from the metadata of the model that was used to described the model.">
   <criteria>ATSelectionCriterion</criteria>
   <criteria>ATSimpleStringCriterion</criteria>
   <criteria>ATListCriterion</criteria>
  </index>
  <index enabled="True" 
      name="pmr2_exposure_workspace" 
      friendlyName="PMR2 Exposure Workspace"
      description="Name of the workspace this exposure is derived from.">
   <criteria>ATSelectionCriterion</criteria>
  </index>
 </topic_indexes>
 <topic_metadata i18n:domain="pmr2.app">
  <metadata name="pmr2_review_state" enabled="True" description=""
      friendlyName="PMR2 Review State"/>
  <metadata name="pmr2_curation" enabled="True" description=""
      friendlyName="PMR2 Curation"/>
  <metadata name="pmr2_keyword" enabled="True" description=""
      friendlyName="PMR2 Model Keyword"/>
  <metadata name="pmr2_exposure_workspace" enabled="False" description=""
      friendlyName="PMR2 Exposure Workspace"/>
  <metadata name="pmr2_exposure_root_title" enabled="False" description=""
      friendlyName="PMR2 Root Title"/>
  <metadata name="pmr2_exposure_root_path" enabled="False" description=""
      friendlyName="PMR2 Root Path"/>
  <metadata name="pmr2_exposure_root_id" enabled="False" description=""
      friendlyName="PMR2 Root ID"/>
 </topic_metadata>
</atcttool>
